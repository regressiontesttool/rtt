Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	b := true;
	i := 3;
	r := 3.5;
	
	Write b = i;
	Write i = b;
	Write b = r;
	Write r = b;
	
	Write b # i;
	Write i # b;
	Write b # r;
	Write r # b;
	
	Write b > b;
	Write b > i;
	Write b > r;
	
	Write b < b;
	Write i < b;
	Write r < b;
	
	Write b >= b;
	Write b >= i;
	Write b >= r;
	
	Write b <= b;
	Write i <= b;
	Write r <= b;
End;
