Procedure main() Begin
	Var x:Integer;
	Var y:Integer;
	
	x := 1;
	y := 2;
	
	Write x;
	Write y;
	
	Begin
		Write x;
		Write y;
		
		Var x:Boolean;
		x := true;
		
		Write x;
		Write y;
		
		Begin
			Write x;
			Write y;
			
			Var y:Boolean;
			y := true;
			
			Write x;
			Write y;
		End;
		
		Write x;
		Write y;
		
		Begin
			Write x;
			Write y;
			
			Var x:Real;
			Var y:Real;
			x := 1.0;
			y := 2.0;
			
			Write x;
			Write y;
		End;
		
		Write x;
		Write y;
	End;
	
	Write x;
	Write y;
End;
